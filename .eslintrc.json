{
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
    "camelcase":"error",
    "no-console": "warn",
    "no-debugger": "warn",
    "no-var":"error",
    "no-trailing-spaces":"error",
    "comma-dangle": ["error", "never"],
    "quotes": ["error", "single"]
  }
}
